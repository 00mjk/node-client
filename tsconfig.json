{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "target": "es6",
    "outDir": "./lib"
  },
  "inclue":[
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "__tests__",
    "examples",
    "lib"
  ]
}
